{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "no-console": "warn",
    "linebreak-style": [
      "error",
      "unix"
    ]
  }
}